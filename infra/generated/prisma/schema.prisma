generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model products {
  id    Int     @id @default(autoincrement())
  name  String  @unique
  desc  String?
  price Decimal @db.Decimal(10, 2)
}
